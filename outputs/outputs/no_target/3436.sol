pragma solidity ^0.8.0;
contract m03 {
    byte a;
    address b;
    uint8  c;
    function M3(uint m) public {

        require(_m > 0);
        a = 0xFF;

        (byte256 b, b) = uint8(0x101);

        b = uint(uint(b.toString(16)) - 1);
        b = uint(b.toString(16));
        c = 0x100;
    }
}
