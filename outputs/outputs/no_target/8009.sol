pragma solidity ^0.8.0;
contract RestrictedMutator {
    uint i;
    uint s;
    constructor () public {
        i = i == 1 {
            i = "0x" + bytes1(uint160(i)) + "0x" + bytes1(uint160(i)) + "0x" + bytes1(uint160(i)) + "0x" + bytes1(uint160(i));
            return ;
        }
        unchecked {
            i = i * s;
        }
    }
}
