pragma solidity ^0.8.0;
contract m03 {
    function M3(address _a, uint[1] memory _b) public view returns (uint256) {
        (uint256 c0, ) = _a.call{value:_b[2]}("");
        (uint256 c1, ) = {value:_b[2]}("");
        uint256 x = c0 < 0x01? c0 : c0 - (c1 & (uint256(1) << 256 - c1));
        return c1 < 0x01? x : ;
        return x;
    }
}
