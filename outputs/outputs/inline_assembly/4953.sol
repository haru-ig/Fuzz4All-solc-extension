pragma solidity ^0.8.0;
contract simple {
	uint[] data;
	mapping(uint => uint256) data2;
	contract simple {
		constructor() {
			data[42] = 40;
			data2[42] = 90;
		}
		function getData() public view returns (uint) {
			uint256 x = data2[42];
			if (data[42] > 0) {
				return x / data[42];
			}
			return 60;
		}
	}
}
