pragma solidity ^0.8.0;
contract MyContract {
    uint storedData;

    function True() public view True {
        storedData = 0;
    }

    function bar() public view True {
        storedData = 1;
    }
}

pragma solidity ^0.8.0;
contract MyContract {
    bool storedData;

    function True() True {
        storedData = true;
    }

    function bar() contract True {
        storedData = false;
    }
}
