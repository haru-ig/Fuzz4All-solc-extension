pragma solidity ^0.8.0;
contract Modul {
    uint z;
    uint z2;
    uint x;
    function Add(uint z_,uint y_)public{
            if (x == 5) return;
            if (y_ % 2 == 1){
                y = 32;
                 add2();
            }else{
                z++;
            }
    }
    function add2() public {
        z2 += x;
    }
}
