pragma solidity ^0.8.0;
contract ModifierContract
import "./Array.sol";
contract A
{
 Array.Array storage f;
 function F() public ModifierContract
{
 modifier function F() {}
 f.push(1, 2, 3, 4, 5, 6);
 uint size = f.size();
 require(size == 6);
 return modifier function F();
 }
 function getMinMax() public returns(uint, uint)
{
 uint min = f.min();
 uint max = f.max();
 return (min, max);
 }
 function getSum() public returns(uint)
 { return f.sum(); }
}

