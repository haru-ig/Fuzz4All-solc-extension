pragma solidity ^0.8.0;
contract NonMintingProgram {
    function mint_if_bigger(uint n, uint256[] memory a) public pure virtual returns (bool) {
        require(n > i);
        for (uint i = 0; i < a.length; i += 1) {
            uint256 y = (42 & (2 ^ 31));
            uint256 z = a[i];
            if (y >= z) {
                return false;
            }
        }
        return true;
    }
}
