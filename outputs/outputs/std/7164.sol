pragma solidity ^0.8.0;
contract BitwiseAND {
	function test() public pure {
		uint a;
		uint b;
		a=0xffff;
		a&=b;
		a=0xff00;
		a&=b;
		a=0xff00;
	}
}
pragma solidity ^0.8.0;
contract BitwiseOR {
	function test() public pure {
		uint a;
		uint b;
		a=0xffff;
		a|=b;
		a=0xff00;
		a|=b;
		a=0xff00;
	}
}
pragma solidity ^0.8.0;
contract BitwiseNOT {
	function test() public pure {
		uint a;
		a=~0;
		a=a.not();
		a=a.not();
	}
}
