pragma solidity ^0.8.0;
contract Convert {
    function bytesToUint(bytes memory _buf) public pure returns (uint256) {
        bytes memory buf = _buf;
        uint256 result = 0 ;
        for (uint256 i = 0; i < buf.length; i++) {

            uint256 byte = uint8(buf[i]);

            result |= uint256(byte) << (8 * (buf.length - 1 - i));
        }
        return result;
    }
}

