		uint x;
		}
	S s;
	mapping(uint => I) I;
	constructor() {
		s.x;
		c[0];
		}
	function f(uint i) public {
		s.x;
		s.x = 0;
		I[0].x;
		I[0].x = 0;
		c[0];
		}
	}
