pragma solidity ^0.8.0;
contract Program {
    function swapLeft(uint[] memory arr) public {
        uint[] memory newArr = new uint[](arr.length + 1);

        for (uint i = 0; i < newArr.length;) {
            newArr[i] = arr[i];
            i++;
        }
    }
}
