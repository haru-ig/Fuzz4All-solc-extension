pragma solidity ^0.8.0;
contract FloatAdd
	{
		uint256 x1;
		uint256 x2;
		constructor() public { x1 = 1;}
		function FloatAdd(uint y) public { x2 = x1 = (x2 + (1e7-1)) * y;}}

